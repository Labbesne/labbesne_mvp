<p id="notice"><%= notice %></p>

<h1>Lifestyles</h1>

<table>
  <thead>
    <tr>
      <th>Profiles</th>
      <th>Activity levels</th>
      <th>Works</th>
      <th>Job types</th>
      <th>Work hours</th>
      <th>Studies</th>
      <th>Study hours</th>
      <th>Social activities</th>
      <th>Outdoor levels</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lifestyles.each do |lifestyle| %>
      <tr>
        <td><%= lifestyle.profiles_id %></td>
        <td><%= lifestyle.activity_levels_id %></td>
        <td><%= lifestyle.works %></td>
        <td><%= lifestyle.job_types_id %></td>
        <td><%= lifestyle.work_hours %></td>
        <td><%= lifestyle.studies %></td>
        <td><%= lifestyle.study_hours %></td>
        <td><%= lifestyle.social_activities_id %></td>
        <td><%= lifestyle.outdoor_levels_id %></td>
        <td><%= link_to 'Show', lifestyle %></td>
        <td><%= link_to 'Edit', edit_lifestyle_path(lifestyle) %></td>
        <td><%= link_to 'Destroy', lifestyle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lifestyle', new_lifestyle_path %>
